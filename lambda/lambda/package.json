{
  "name": "lambda-example",
  "version": "1.0.0",
  "description": "AWS Lambda Example",
  "main": "index.js",
  "repository": "git@github.com:Josaber/lambda-example.git",
  "author": "younever4ever@gmail.com",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.257.1"
  },
  "devDependencies": {
    "jest": "^23.1.0",
    "json-server": "^0.12.2"
  },
  "scripts": {
    "start": "node index.js",
    "style": "",
    "test": "",
    "mockserver": "json-server --watch --port 4000 api/index.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
